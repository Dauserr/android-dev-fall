<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_background"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingStart="@dimen/screen_margin"
        android:paddingEnd="@dimen/screen_margin">
        <SearchView
            android:id="@+id/search_bar"
            android:theme="@style/MySearchView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:iconifiedByDefault="false"
            android:queryHint="Search apps &amp; games"
            android:imeOptions="actionSearch"
            android:inputType="text"
            android:background="@drawable/search_bar_bg"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/profilePicture"/>
        <ImageView
            android:id="@+id/profilePicture"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@mipmap/ic_launcher"
            android:scaleType="centerCrop"
            android:background="@drawable/circle_shape"
            android:clipToOutline="true"
            android:layout_marginTop="4dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/exploreText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Explore games"
            android:textColor="@color/text_primary"
            android:textSize="20sp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            app:layout_constraintTop_toBottomOf="@id/search_bar"
            app:layout_constraintStart_toStartOf="parent"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesAction"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/exploreText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/gamesSimulation">

            <TextView
                android:id="@+id/textAction"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Action"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <ImageView
                android:id="@+id/iconAction"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesSimulation"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/exploreText"
            app:layout_constraintStart_toEndOf="@+id/gamesAction"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textSimulation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Simulation"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconSimulation"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesPuzzle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesAction"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/gamesSimulation">

            <TextView
                android:id="@+id/textPuzzle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Puzzle"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconPuzzle"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesAdventure"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesAction"
            app:layout_constraintStart_toEndOf="@+id/gamesPuzzle"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textAdventure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Adventure"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconAdventure"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesRacing"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesPuzzle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/gamesSimulation">

            <TextView
                android:id="@+id/textRacing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Racing"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconRacing"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesRolePlaying"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesAdventure"
            app:layout_constraintStart_toEndOf="@+id/gamesRacing"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textRolePlaying"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Role Playing"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconRolePlaying"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesStrategy"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesRacing"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/gamesSimulation">

            <TextView
                android:id="@+id/textStrategy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Strategy"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconStrategy"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesSports"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesRolePlaying"
            app:layout_constraintStart_toEndOf="@+id/gamesStrategy"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textSports"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sports"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconSports"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesCard"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesStrategy"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/gamesSimulation">

            <TextView
                android:id="@+id/textCard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Card"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconCard"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesBoard"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesSports"
            app:layout_constraintStart_toEndOf="@+id/gamesCard"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textBoard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Board"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconBoard"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/sponsoredText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sponsored · "
            android:textColor="@color/text_primary"
            android:textSize="20sp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/gamesCard"
            app:layout_constraintStart_toStartOf="parent"
            />

        <TextView
            android:id="@+id/suggestedText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Suggested for you"
            android:textColor="@color/text_primary"
            android:textSize="25sp"
            android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@id/gamesCard"
            app:layout_constraintStart_toEndOf="@id/sponsoredText"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/m3_split_button_chevron_avd"
            android:layout_marginTop="25dp"
            android:layout_marginRight="10dp"
            app:layout_constraintTop_toBottomOf="@id/gamesBoard"
            app:layout_constraintEnd_toEndOf="parent"
            android:clickable="true"
            android:tint="@color/text_primary"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/suggestedGame"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            android:padding="10dp"
            app:layout_constraintTop_toBottomOf="@+id/sponsoredText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/iconSuggestedGame"
                android:layout_height="72dp"
                android:layout_width="72dp"
                android:layout_margin="5dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />


            <TextView
                android:id="@+id/textSuggestedGame"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Clash of Clans"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                app:layout_constraintTop_toTopOf="@id/suggestedGame"
                app:layout_constraintStart_toEndOf="@id/iconSuggestedGame" />

            <TextView
                android:id="@+id/genresSuggestedGame"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Supercell · Strategy · Build &amp; battle \n4.6★  363MB  500M+"
                android:textColor="@color/text_secondary"
                android:textSize="15sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintTop_toBottomOf="@id/textSuggestedGame"
                app:layout_constraintStart_toEndOf="@id/iconSuggestedGame" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/exploreSecondText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Explore apps"
            android:textColor="@color/text_primary"
            android:textSize="20sp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            app:layout_constraintTop_toBottomOf="@id/suggestedGame"
            app:layout_constraintStart_toStartOf="parent"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/appsEntertainment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/exploreSecondText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/gamesSimulation">

            <TextView
                android:id="@+id/textEntertainment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Entertainment"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <ImageView
                android:id="@+id/iconEntertainment"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesSocial"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/exploreSecondText"
            app:layout_constraintStart_toEndOf="@+id/gamesAction"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textSocial"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Social"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconSocial"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/appsProductivity"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/appsEntertainment"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/gamesSimulation">

            <TextView
                android:id="@+id/textProductivity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Productivity"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <ImageView
                android:id="@+id/iconProductivity"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/gamesCommunication"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg"
            android:layout_margin="2dp"
            app:layout_constraintTop_toBottomOf="@+id/gamesSocial"
            app:layout_constraintStart_toEndOf="@+id/gamesAction"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textCommunication"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Communication"
                android:textColor="@color/text_primary"
                android:textSize="20sp"
                android:layout_marginLeft="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:id="@+id/iconCommunication"
                android:layout_height="48dp"
                android:layout_width="48dp"
                android:layout_margin="20dp"
                android:src="@mipmap/ic_launcher"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#1E1F21"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"
        android:clipToPadding="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:menu="@menu/bottom_nav_menu"
        app:labelVisibilityMode="labeled"
        app:itemIconTint="#BCBDBF"
        app:itemTextColor="#FFFFFF"
        app:itemTextAppearanceActive="@style/BottomNavText"
        app:itemTextAppearanceInactive="@style/BottomNavText"/>

</androidx.constraintlayout.widget.ConstraintLayout>


